<src:視窗繼承 x:Class="DarbWareERP.B.基本資料.單位轉換表"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DarbWareERP.B.基本資料"
        xmlns:src="clr-namespace:DarbWareERP.繼承窗口"
          xmlns:控制項="clr-namespace:DarbWareERP.控制項" xmlns:導覽區="clr-namespace:DarbWareERP.控制項.導覽區"
        mc:Ignorable="d" Height="768" Width="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="259*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Border BorderThickness="3" Grid.Row="1">
            <Border.BorderBrush>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.InactiveCaptionColorKey}}"/>
            </Border.BorderBrush>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="656*"/>
                </Grid.ColumnDefinitions>
                <控制項:命令區塊 Margin="0,14,0,0.6" HorizontalAlignment="Left" Width="95" Grid.Column="0"/>
                <導覽區:B_基本資料 HorizontalAlignment="Left" VerticalAlignment="Top" Width="244" Grid.Column="1"/>
                <導覽區:導覽區 Grid.Column="2" HorizontalAlignment="Left" Margin="9.8,1,0,0" VerticalAlignment="Top" Width="244"/>
            </Grid>
        </Border>

    </Grid>
</src:視窗繼承>
