        xmlns:local="clr-namespace:DarbWareERP"
        xmlns:src="clr-namespace:DarbWareERP.繼承窗口"   
        mc:Ignorable="d"
        Height="768" Width="1024" WindowState="Maximized" Loaded="視窗繼承_Loaded" Tag="選單">
    <Grid>
        <Grid Height="655" Margin="68,38,69.6,45.4" Width="880" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Grid.Background>
                <ImageBrush ImageSource="Image/Icon_png/選單頁底圖.png"/>
            </Grid.Background>
            <Label x:Name="label2" Content="Darbware" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="64" FontWeight="Bold" Foreground="#FFE6B43C" Margin="0,15,0,0"/>
            <Button x:Name="btn電子賀卡" Content="電子賀卡" Margin="10,447,0,0" HorizontalAlignment="Left" Width="90" Height="40" VerticalAlignment="Top" Click="btn電子賀卡_Click"/>
