            <Button x:Name="btn返回登入視窗" HorizontalAlignment="Center" Margin="668,576,52,25" VerticalAlignment="Center" Width="160" Height="55
                    " FontSize="16" Click="btn返回登入視窗_Click">
                <StackPanel Orientation="Horizontal">
                    <Image Source="Image/Icon_png/取消00.png" Stretch="None"/>
                    <TextBlock Margin="3" HorizontalAlignment="Center" VerticalAlignment="Center">回到登入視窗</TextBlock>

                </StackPanel>
            </Button>
            <Button x:Name="btn轉ACCL" Content="轉ACCL" Margin="200,447,0,0" HorizontalAlignment="Left" Width="90" Height="40" VerticalAlignment="Top" Click="btn轉ACCL_Click" />
            <Button x:Name="btn轉sliy" Content="轉Sliy" Margin="295,447,0,0" HorizontalAlignment="Left" Width="90" Height="40" VerticalAlignment="Top" Click="btn轉sliy_Click"  />
            <WrapPanel Name="WrapPanel" Height="104" Margin="0,102,0,0" VerticalAlignment="Top" Orientation="Vertical" >
                <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Height="40" Margin="3"/>
                <Button x:Name="button2" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="90" Height="40" Margin="3"/>
